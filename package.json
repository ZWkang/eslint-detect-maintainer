    "test": "vitest run",
    "build": "tsup",
    "start": "tsx src/index.ts",
    "release": "bumpp && npm publish",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.33.1",
